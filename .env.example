# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Firebase Cloud Messaging (FCM)
# Generate VAPID key in Firebase Console > Project Settings > Cloud Messaging
# See docs/FCM_SETUP.md for detailed instructions
VITE_FIREBASE_VAPID_KEY=your-vapid-key-here

# Sentry Configuration
VITE_SENTRY_DSN=your-sentry-dsn
VITE_SENTRY_PROJECT=your-sentry-project
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=your-sentry-project
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# App Configuration
VITE_APP_VARIANT=production  # or 'nightly'

# Google Analytics
VITE_GA_MEASUREMENT_ID=your-ga-measurement-id
